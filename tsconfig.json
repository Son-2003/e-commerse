{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["dom", "es2017"],
    "module": "ESNext",
    "jsx": "react-jsx",
    "baseUrl": "src",
    "paths": {
      "services/*": ["services/*"],
      "common/*": ["common/*"],
      "@redux/*": ["redux/*"],
      "components/*": ["components/*"],
      "pages/*": ["pages/*"]
    },
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
